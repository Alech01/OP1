local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Key system Join to disk ", HidePremium = false, SaveConfig = false, introEnable = false})

OrionLib:MakeNotification({
	Name = "Key tags",
	Content = "TYMS FOR USE MY SCRIPT",
	Image = "rbxassetid://4483345998",
	Time = 5
})
_G.key = "MNWFNAJFNAFWF9AN2"
_G.keyImput = "String"

local Tab = Window:MakeTab({
	Name = "Key System",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Key Correct",
        Content = "You Have Accest for my script",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function inCorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "inKey Correct",
        Content = "You  no Have Accest for my script, Please put The key correct",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end
Tab:AddTextbox({
	Name = "Key",
	Default = " Join Disk For key: https://discord.gg/35gXPCBmsD",
	TextDisappear = true,
	Callback = function(Value)
		_G.keyImput = Value
	end	  
})
Tab:AddButton({
	Name = "Enter Key",
	Callback = function()
    if _G.keyImput == _G.key then
        MakeScriptHub()
        CorrectKeyNotification()
    else
        inCorrectKeyNotification()
        end
  	end    
})
function MakeScriptHub()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/Alech01/op-/main/Op%20Script%20Push%20Simulator')))()
end
